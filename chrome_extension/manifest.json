{
  "name": "UdemyScrap",
  "description": "Scraping Udemy course info",
  "version": "0.1",
  "manifest_version": 3,
  "host_permissions": ["http://localhost/*"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.udemy.com/course/*"],
      "js": ["js/content-script.js"]
    }
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": ["tabs"]
}
